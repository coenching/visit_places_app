<h1>Existing Place</h1>

<div style="display:inline;float:left;">
	<p>Name: <%=@place.name%></p>
	<p>Address: <%=@place.address%></p>
	<p>Type: <%=@place.gender%></p>
	<p>Price: <%=@place.price%></p>
	<input type='hidden' id='place_lat' value="<%=@place.lat%>">
	<input type='hidden' id='place_lng' value="<%=@place.lng%>">

	<%= link_to 'Not that place, create a new one', new_place_path%>

	<%= render 'visits/form' %>
</div>

<div style="display:inline;float:left;">
  <div id="map-canvas" data-map="disable"></div>
</div>
<script type="text/javascript">
google.maps.event.addDomListener(window, 'load', initialize);
</script>
