<div class="row">
  	<div class="col-md-7">
		<div class="well place">
			<%= image_tag(place.picture_url, height: 160, class: "img-rounded pull-right") if place.picture.present? %>
			<p><strong>Place name: </strong><%=place.name%></p>
			<p><strong>Address: </strong><%=place.address%></p>
			<p><strong>Type: </strong><%=place.gender%></p>
			<p><strong>Price: </strong><%=place.price%></p>
			<input type='hidden' id='place_lat' value="<%=place.lat%>">
			<input type='hidden' id='place_lng' value="<%=place.lng%>">
		</div>
	</div>
	<div class="col-md-5">
	    <div id="map-canvas" data-map="disable"></div>
	</div>
</div>

<script type="text/javascript">
google.maps.event.addDomListener(window, 'load', initialize);
</script>